{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["./docsource"],
    "includePattern": ".js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "private": true,
    "template": "./node_modules/jsdoc-docio/template",
    "destination": "./docs/"
  },
  "plugins": [
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "./node_modules/jsdoc-docio/template",

    "systemName"            : "OB Suit API",
    "footer"                : "",
    "copyright"             : "OB Â© 2019",
    "includeDate"           : true,
    "navType"               : "inline",
    "theme"                 : "docio",
    "syntaxTheme"           : "dark",
    "inverseNav"            : true,
    "linenums"              : true,
    "dateFormat"            : "YYYY-MM-DD, h:mm:ss a"
  }
}
